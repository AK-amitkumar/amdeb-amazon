{% extends "amazon_envelope.jj2" %}

{% block product_feed %}
    <MessageType>Inventory</MessageType>
    {% for message in FeedMessages %}
    <Message>
        <MessageID>{{ message['ID'] }}</MessageID>
        <OperationType>Update</OperationType>
        <Inventory>
            <SKU>{{ message['SKU'] }}</SKU>
            <Quantity>{{ message['quantity'] }}</Quantity>
        </Inventory>
    </Message>
    {% endfor %}
{% endblock %}